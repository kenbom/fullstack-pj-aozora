generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShiwakePtn {
  id           Int      @id @default(autoincrement())
  shiwakePtn   Int
  kariCd       Int
  karikKingaku Int
  kariKubun    Boolean
  kashiCd      Int
  kashiKingaku Int
  kashiKubun   Boolean
  hasseiDate   DateTime
  userId       Int
  createdAt    DateTime @default(now())
}
